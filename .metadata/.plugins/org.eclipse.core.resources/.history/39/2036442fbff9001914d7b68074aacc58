package tp5ej2;

import java.util.ArrayList;

public class Buzon {
protected ArrayList<Carta> cartas;
private ArrayList<Niño> buenos;

public Buzon(){
	cartas = new ArrayList <Carta>();
	buenos = new ArrayList <Niño>();
}

public void add(Carta r){
	cartas.add(r);
}
public void add(Niño r){
	buenos.add(r);
}
}
